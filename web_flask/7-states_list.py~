#!/usr/bin/python3
"""
'7-states_list' is a script that starts a Flask Web Application
"""
from models import *
from models import storage
from flask import Flask, render_template
import os
from models.states import State


# Sets this script to be the controller in this Framework
app = Flask(__name__)


# Definitions for associated routes
@app.route("/states_list", strict_slashes=False)
def state_list():
    """Function called when the '/state_list' route is accessed."""
    state_list = storage.all(State)
    return (render_template("7-states_list.html", states=state_list))
